\name{Kmeans}
\alias{Kmeans}
\title{Title}
\usage{
Kmeans(data.mat, centers.mat)
}
\arguments{
\item{data.mat}{numeric matrix of input data [n x p]}

\item{centers.mat}{numeric matrix of randomly starting cluster centers with the rows corresponding the amount of desired clusters [k x p]}
}
\value{
numeric vector of cluster assignments [1 x n]
}
\description{
Title
}
\examples{
data.mat <- as.matrix(iris[, c("Petal.Length", "Petal.Width")])
K <- 3
centers.mat <- data.mat[sample(1:nrow(data.mat), K), ]
cluster.vec <- RPackage1::KMeans_interface(data.mat, centers.mat)
}
